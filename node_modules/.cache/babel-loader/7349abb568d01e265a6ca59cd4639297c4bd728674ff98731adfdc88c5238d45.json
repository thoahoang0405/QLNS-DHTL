{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Paginate from \"vuejs-paginate-next\";\nimport Form from \"../base/FormDetail.vue\";\n// import PopupConfirm from \"../base/BasePopupDelete.vue\"\nimport PopupConfirm from \"../base/deleteStudent.vue\";\nimport $ from \"jquery\";\nimport Combobox from \"../base/BaseCombobox2.vue\";\nimport axios from \"axios\";\nimport { useToast } from \"vue-toastification\";\nexport default {\n  components: {\n    Paginate,\n    Form,\n    PopupConfirm,\n    Combobox\n  },\n  data() {\n    return {\n      isActive: \"20\",\n      studentSelected: [],\n      pageNumber: 1,\n      page: 1,\n      totalPage: 0,\n      isShowConfirm: false,\n      isShowForm: false,\n      isShow: false,\n      //gán v-show=isShow hoặc v-show =false để ẩn form\n      isShowDrop: false,\n      students: {},\n      pageDefault: 20,\n      msvDelete: \"\",\n      faculty: {},\n      facultyID: \"\",\n      classification: {},\n      classificationID: \"\",\n      txtSearch: \"\",\n      newCode: \"\",\n      formMode: 1\n    };\n  },\n  watch: {\n    txtSearch: function () {\n      if (this.txtSearch == \"\") {\n        this.getpagingStudent();\n      }\n    }\n  },\n  created() {\n    this.getpagingStudent();\n    this.getDepartment();\n    this.getPosition();\n  },\n  methods: {\n    deleteClick(item) {\n      this.stuCodeDelete = item.StudentCode;\n      this.msvDelete = item.StudentID;\n      console.log(this.stuCodeDelete);\n      this.isShowConfirm = !this.isShow;\n\n      // console.log(item.msv);\n      //sao v\n    },\n\n    deleteStu(value) {\n      this.isShowConfirm = value;\n      var me = this;\n      const toast = useToast();\n      try {\n        axios.delete(`https://localhost:7029/api/Students/${me.msvDelete}`).then(function (res) {\n          console.log(res);\n          toast.success(\"Xóa dữ liệu thành công\", {\n            timeout: 2000\n          });\n          me.getpagingStudent();\n        }).catch(function () {\n          toast.error(\"xóa dữ liệu thất bại\", {\n            timeout: 2000\n          });\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    getpagingStudent() {\n      try {\n        var me = this;\n        axios.get(`https://localhost:7029/api/Students/Filter?keyword=${this.txtSearch}&pageSize=${this.pageDefault}&facultyID=${this.facultyID}&classificationID=${this.classificationID}&pageNumber=${this.pageNumber}`).then(function (res) {\n          me.totalPage = res.data.TotalPages;\n          me.totalRecord = res.data.TotalRecords;\n          me.students = res.data.Data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    gender(gender) {\n      switch (gender) {\n        case 0:\n          gender = \"Nam\";\n          break;\n        case 1:\n          gender = \"Nữ\";\n          break;\n        default:\n          break;\n      }\n      return gender;\n    },\n    getDepartment() {\n      try {\n        var me = this;\n        axios.get(\"https://localhost:7029/api/Faculty\").then(function (res) {\n          me.faculty = res.data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    getPosition() {\n      try {\n        var me = this;\n        axios.get(\"https://localhost:7029/api/Classifications\").then(function (res) {\n          me.classification = res.data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    formatDate(date) {\n      try {\n        if (date) {\n          date = new Date(date);\n          let newDate = date.getDate();\n          let month = date.getMonth() + 1;\n          let year = date.getFullYear();\n          newDate = newDate < 10 ? `0${newDate}` : newDate;\n          month = month < 10 ? `0${month}` : month;\n          return `${newDate}-${month}-${year}`;\n        }\n      } catch (error) {\n        return \"\";\n      }\n    },\n    selectItemCbb(value) {\n      if (value.FacultyID) {\n        this.facultyID = value.FacultyID;\n      } else {\n        this.facultyID = \"\";\n        this.getpagingStudent();\n      }\n      this.getpagingStudent();\n    },\n    selectItemCategory(value) {\n      console.log(value.ClassificationID);\n      if (value.ClassificationID) {\n        this.classificationID = value.ClassificationID;\n      } else {\n        this.classificationID = \"\";\n        this.getpagingStudent();\n      }\n      this.getpagingStudent();\n    },\n    clickAdd(item) {\n      this.list.push(item);\n    },\n    showPage(is) {\n      this.isShowDrop = is;\n    },\n    btnAddStudent() {\n      this.isShowForm = !this.isShowForm;\n      this.formMode = 1;\n      this.getNewCode();\n    },\n    getNewCode() {\n      try {\n        var me = this;\n        axios.get(\"https://localhost:7029/api/Students/NewCode\").then(function (res) {\n          me.newCode = res.data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    editStudent(item) {\n      this.studentID = item.StudentID;\n      console.log(item.StudentID);\n      this.formMode = 2;\n      this.isShowForm = !this.isShowForm;\n      this.studentSelected = item;\n      this.formMode = 2;\n    },\n    cancelConfirm(value) {\n      this.isShowConfirm = value;\n    },\n    //nhận lệnh ẩn từ bên form chi tiết\n    closeFormDetail(value) {\n      console.log(value);\n      this.isShow = value;\n    },\n    hideFormDetail(value) {\n      this.isShowForm = value;\n    },\n    btnDropUp() {\n      $(\".icon-dropup\").toggleClass(\"iconrotate\");\n      if (!this.isShowDrop) {\n        this.showPage(true);\n        $(\".icon-dropup\").addClass(\"iconrotate\");\n      } else {\n        this.showPage(false);\n        $(\".icon-dropup\").removeClass(\"iconrotate\");\n      }\n    },\n    getPageDefault(e) {\n      this.isActive = e.target.getAttribute(\"pageSize\");\n      this.pageDefault = e.target.getAttribute(\"pageSize\");\n      this.showPage(false);\n      $(\".icon-dropup\").removeClass(\"iconrotate\");\n      this.getpagingStudent();\n      if (this.pageDefault > this.totalRecord) {\n        this.pageDefault = this.totalRecord;\n      }\n    },\n    clickCallback(pageNum) {\n      this.pageNumber = pageNum;\n      this.getpagingStudent();\n    }\n  }\n};","map":{"version":3,"names":["Paginate","Form","PopupConfirm","$","Combobox","axios","useToast","components","data","isActive","studentSelected","pageNumber","page","totalPage","isShowConfirm","isShowForm","isShow","isShowDrop","students","pageDefault","msvDelete","faculty","facultyID","classification","classificationID","txtSearch","newCode","formMode","watch","getpagingStudent","created","getDepartment","getPosition","methods","deleteClick","item","stuCodeDelete","StudentCode","StudentID","console","log","deleteStu","value","me","toast","delete","then","res","success","timeout","catch","error","get","TotalPages","totalRecord","TotalRecords","Data","gender","formatDate","date","Date","newDate","getDate","month","getMonth","year","getFullYear","selectItemCbb","FacultyID","selectItemCategory","ClassificationID","clickAdd","list","push","showPage","is","btnAddStudent","getNewCode","editStudent","studentID","cancelConfirm","closeFormDetail","hideFormDetail","btnDropUp","toggleClass","addClass","removeClass","getPageDefault","e","target","getAttribute","clickCallback","pageNum"],"sources":["D:\\QLNS\\QLNS-DHTL\\src\\components\\page\\StudentManag.vue"],"sourcesContent":["<template>\r\n  <div class=\"body\">\r\n    <h3 class=\"header-bd\">Quản lý sinh viên</h3>\r\n    <div class=\"body-header\">\r\n      <div class=\"row-input\">\r\n        <div class=\"input\">\r\n          <input type=\"text\" class=\"search\" v-model=\"txtSearch\"  @keypress.enter=\"getpagingStudent\" placeholder=\"Tìm kiếm sinh viên\" />\r\n          <div class=\"icon-search icon\"></div>\r\n        </div>\r\n        <Combobox\r\n          :items=\"faculty\"\r\n         \r\n          :fieldName=\"'FacultyName'\"\r\n         \r\n          @selectedItem=\"selectItemCbb\"\r\n        ></Combobox>\r\n      \r\n        <Combobox\r\n          :items=\"classification\"       \r\n          :fieldName=\"'Classification'\"\r\n          @selectedItem=\"selectItemCategory\"\r\n         \r\n        ></Combobox>\r\n      </div>\r\n      <div class=\"button-function\">\r\n        <div class=\"add\">\r\n          <button @click=\"btnAddStudent\" class=\"btn-add btn-hover-blue\">+ Thêm sinh viên</button>\r\n        </div>\r\n\r\n        <button @click=\"getpagingStudent\" class=\"btn-excel\">\r\n          <div class=\"icon icon-load\">\r\n            <div class=\"tooltip-excel\">Lấy lại dữ liệu</div>\r\n          </div>\r\n        </button>\r\n        \r\n      </div>\r\n    </div>\r\n    <div id=\"m-table\" class=\"m-table\">\r\n      <table id=\"tbEmployee\" class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th\r\n              class=\"sticky-left-top checkbox\"\r\n              colspan=\"1\"\r\n              style=\"min-width: 30px !important; text-align: center\"\r\n            >\r\n              <input\r\n                ref=\"checkall\"\r\n                type=\"checkBox\"\r\n                class=\"check-all\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </th>\r\n            <th class=\"text-center\" style=\"min-width: 40px;\">STT</th>\r\n            <th>Mã sinh viên</th>\r\n            <th>Họ và tên</th>\r\n            <th>Giới tính</th>\r\n            <th>Ngày sinh</th>\r\n            <th>Số CMND/CCCD</th>\r\n            <th>Email</th>\r\n            <th>Số điện thoại</th>\r\n            <th>Số tài khoản</th>\r\n            <th>Tên ngân hàng</th>\r\n            <th>Địa chỉ</th>\r\n            <th>Khoa</th>\r\n            <th>Lớp</th>\r\n            <th>Chương trình đào tạo</th>\r\n            <th>Xếp loại</th>\r\n            <th>Trạng thái</th>\r\n           \r\n          \r\n            <th\r\n              class=\"th-item-final sticky-right-top\"\r\n              colspan=\"12\"\r\n              style=\"min-width: 100px; text-align: center\"\r\n            >\r\n              <div class=\"th-item\">\r\n                <span class=\"table-text\">Chức năng </span>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr ref=\"row\" v-for=\"(item,index) in students\" :key=\"item.StudentCode\" @dblclick=\"editStudent(item)\" >\r\n            <td\r\n              ref=\"rowCheck\"\r\n              class=\"checkbox sticky-left\"\r\n              style=\"text-align: center\"\r\n              colspan=\"1\"\r\n            >\r\n              <input\r\n                ref=\"check\"\r\n                class=\"check-item\"\r\n                type=\"checkbox\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </td>\r\n            <td class=\"text-center\">{{ index+1 }}</td>\r\n            <td>{{ item.StudentCode }}</td>\r\n            <td>{{ item.StudentName }}</td>\r\n            <td>{{ gender(item.Gender) }}</td>\r\n            <td>{{ formatDate(item.DateOfBirth) }}</td>\r\n             <td>{{ item.IdentityNumber }}</td>\r\n            <td>{{ item.Email }}</td>\r\n            <td>{{ item.Phonenumber }}</td>\r\n            <td>{{ item.BankAccountNumber }}</td>\r\n            <td>{{ item.BankAccountName}}</td>\r\n            <td>{{ item.Adress }}</td>\r\n            <td>{{ item.FacultyName }}</td>\r\n            <td>{{ item.Class}}</td>\r\n            <td>{{ item.EducationProgramName }}</td>       \r\n            <td>{{ item.ClassificationName }}</td>       \r\n            <td>{{ item.StatusName }}</td>\r\n           \r\n            <td\r\n              ref=\"func\"\r\n              class=\"td-item-final td-func sticky-right\"\r\n              style=\"position: sticky; right: 0; background-color: #fff\"\r\n              colspan=\"12\"\r\n            >\r\n              <div class=\"edit-text\"></div>\r\n              <div class=\"icon icon-edit\" @click=\"editStudent(item)\"></div>\r\n                <div class=\"icon icon-delete\" @click=\"deleteClick(item)\"></div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <div class=\"paging\">\r\n      <div class=\"paging-left\">\r\n        Tổng số:\r\n        <strong>{{ totalRecord }}</strong>\r\n        bản ghi\r\n      </div>\r\n      <div class=\"paging-right\">\r\n        <div class=\"page\">\r\n          <div class=\"content-page\">{{ pageDefault }} bản ghi trên 1 trang</div>\r\n          <div class=\"dropup-page\">\r\n            <div class=\"icon-dropup\" @click=\"btnDropUp\"></div>\r\n            <div class=\"item-up\" v-show=\"isShowDrop\">\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '10' }\"\r\n                pageSize=\"10\"\r\n              \r\n                @click=\"getPageDefault\"\r\n              >\r\n                10 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '20' }\"\r\n                pageSize=\"20\"\r\n                @click=\"getPageDefault\"\r\n                :value=\"pageDefault\"\r\n              >\r\n                20 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '30' }\"\r\n                pageSize=\"30\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                30 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '50' }\"\r\n                pageSize=\"50\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                50 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '100' }\"\r\n                pageSize=\"100\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                100 bản ghi trên 1 trang\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <Paginate\r\n            v-model=\"page\"\r\n            :page-count=\"totalPage\"\r\n            :page-range=\"3\"\r\n            :margin-pages=\"1\"\r\n            :click-handler=\"clickCallback\"\r\n            :prev-text=\"'Trước'\"\r\n            :next-text=\"'Sau'\"\r\n            :container-class=\"'pagination'\"\r\n            :page-class=\"'page-item'\"\r\n          >\r\n          </Paginate>\r\n        </div>\r\n      </div>\r\n    </div>\r\n   \r\n  </div>\r\n<Form v-show=\"isShowForm\" :FormMode=\"formMode\" @hideForm=\"hideFormDetail\" :code=\"newCode\" :loadData=\"getpagingStudent\" :student=\"studentSelected\"></Form>\r\n<PopupConfirm :masv=\"stuCodeDelete\" v-show=\"isShowConfirm\" @closeNotifi=\"deleteStu\"> </PopupConfirm>\r\n<!-- <PopupConfirm :msv=\"msvDelete\" v-show=\"isShowConfirm\" @cancelNotifi=\"cancelConfirm\"> </PopupConfirm> -->\r\n\r\n</template>\r\n<style scoped>\r\n  .btn-add:hover {\r\n    opacity: 0.8;\r\n  }\r\n  \r\n</style>\r\n<script>\r\nimport Paginate from \"vuejs-paginate-next\";\r\nimport Form from \"../base/FormDetail.vue\"\r\n// import PopupConfirm from \"../base/BasePopupDelete.vue\"\r\nimport PopupConfirm from \"../base/deleteStudent.vue\"\r\nimport $ from \"jquery\";\r\nimport Combobox from \"../base/BaseCombobox2.vue\";\r\nimport axios from \"axios\";\r\nimport { useToast } from \"vue-toastification\";\r\n\r\nexport default {\r\n  components: {\r\n    Paginate,Form,PopupConfirm,Combobox\r\n  },\r\n  data() {\r\n    return {\r\n      isActive: \"20\",\r\n      studentSelected: [],\r\n      pageNumber: 1,\r\n      page: 1,\r\n      totalPage: 0,\r\n      isShowConfirm: false,\r\n      isShowForm: false,\r\n      isShow: false, //gán v-show=isShow hoặc v-show =false để ẩn form\r\n      isShowDrop: false,\r\n      students:{},\r\n      pageDefault: 20,\r\n      msvDelete: \"\",\r\n      faculty:{},\r\n      facultyID: \"\",\r\n      classification:{},\r\n      classificationID:\"\",\r\n      txtSearch:\"\",\r\n      newCode: \"\",\r\n      formMode: 1,\r\n    };\r\n  },\r\n  watch: {\r\n    txtSearch: function () {\r\n      if (this.txtSearch == \"\") {\r\n        this.getpagingStudent();\r\n      }\r\n    },\r\n  },\r\n  created(){\r\n    this.getpagingStudent()\r\n    this.getDepartment()\r\n    this.getPosition()\r\n  },\r\n  methods: {\r\n    deleteClick(item){\r\n      this.stuCodeDelete= item.StudentCode\r\n      this.msvDelete=item.StudentID\r\n      console.log(this.stuCodeDelete);\r\n      this.isShowConfirm=!this.isShow;\r\n     \r\n      // console.log(item.msv);\r\n      //sao v\r\n\r\n     \r\n    },\r\n    deleteStu(value){\r\n      this.isShowConfirm=value\r\n      var me = this;\r\n      const toast = useToast();\r\n      try{\r\n       axios\r\n         .delete(\r\n          `https://localhost:7029/api/Students/${me.msvDelete}`\r\n         )\r\n         .then(function (res) {\r\n          console.log(res);\r\n          toast.success(\"Xóa dữ liệu thành công\", { timeout: 2000 });\r\n          me.getpagingStudent()\r\n         })\r\n        \r\n         .catch(function () {\r\n          toast.error(\"xóa dữ liệu thất bại\", { timeout: 2000 });\r\n           console.log(1);\r\n         });\r\n     } catch (error) {\r\n       console.log(error);\r\n     }\r\n    },\r\n\r\n    getpagingStudent() {\r\n      try {\r\n       \r\n        var me = this;\r\n      