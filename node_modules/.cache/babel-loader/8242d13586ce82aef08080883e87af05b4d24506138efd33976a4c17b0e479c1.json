{"ast":null,"code":"import axios from \"axios\";\nimport $ from \"jquery\";\nimport Paginate from \"vuejs-paginate-next\";\nimport { useToast } from \"vue-toastification\";\nexport default {\n  data() {\n    return {\n      isOpenPopupBonus: false,\n      isOpenPopupPunish: false,\n      pageDefault: 10,\n      pageNumber: 1,\n      txtSearch: \"\",\n      totalPage: 1,\n      totalRecord: 0,\n      students: {},\n      isShowDrop: false,\n      dataSinhVienPunish: [],\n      dataSinhVienBonus: {}\n    };\n  },\n  components: {\n    Paginate\n  },\n  watch: {\n    txtSearch: function () {\n      if (this.txtSearch == \"\") {\n        this.getpagingStudent();\n      }\n    }\n  },\n  created() {\n    this.getpagingStudent();\n  },\n  methods: {\n    SavePunish() {\n      var me = this;\n      const toast = useToast();\n      try {\n        axios.post(\"https://localhost:7029/api/Discipline\", this.dataSinhVienPunish).then(function (res) {\n          console.log(res);\n          toast.success(\"thêm dữ liệu thành công\", {\n            timeout: 2000\n          });\n          me.isOpenPopupPunish = false;\n        }).catch(function () {\n          console.log(\"error\");\n          toast.error(\"thêm dữ liệu thất bại\", {\n            timeout: 2000\n          });\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    SaveBonus() {\n      var me = this;\n      const toast = useToast();\n      try {\n        axios.post(\"https://localhost:7029/api/Bonus\", this.dataSinhVienBonus).then(function (res) {\n          console.log(res);\n          toast.success(\"thêm dữ liệu thành công\", {\n            timeout: 2000\n          });\n          me.isOpenPopupBonus();\n        }).catch(function () {\n          console.log(\"error\");\n          toast.error(\"thêm dữ liệu thất bại\", {\n            timeout: 2000\n          });\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    thongbao() {\n      alert(\"Xác nhận đóng\");\n    },\n    OpenPopupBonus(item) {\n      this.isOpenPopupBonus = true;\n      this.dataSinhVienBonus = item;\n    },\n    OpenPopupPunish(item) {\n      this.isOpenPopupPunish = true;\n      this.dataSinhVienPunish = item;\n    },\n    getpagingStudent() {\n      try {\n        var me = this;\n        axios.get(`https://localhost:7029/api/Students/Filter?keyword=${this.txtSearch}&pageSize=${this.pageDefault}&facultyID=${this.facultyID}&classificationID=${this.classificationID}&pageNumber=${this.pageNumber}`).then(function (res) {\n          me.totalPage = res.data.TotalPages;\n          me.totalRecord = res.data.TotalRecords;\n          me.students = res.data.Data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    showPage(is) {\n      this.isShowDrop = is;\n    },\n    btnDropUp() {\n      $(\".icon-dropup\").toggleClass(\"iconrotate\");\n      if (!this.isShowDrop) {\n        this.showPage(true);\n        $(\".icon-dropup\").addClass(\"iconrotate\");\n      } else {\n        this.showPage(false);\n        $(\".icon-dropup\").removeClass(\"iconrotate\");\n      }\n    },\n    getPageDefault(e) {\n      this.isActive = e.target.getAttribute(\"pageSize\");\n      this.pageDefault = e.target.getAttribute(\"pageSize\");\n      this.showPage(false);\n      $(\".icon-dropup\").removeClass(\"iconrotate\");\n      this.getpagingStudent();\n      if (this.pageDefault > this.totalRecord) {\n        this.pageDefault = this.totalRecord;\n      }\n    },\n    clickCallback(pageNum) {\n      this.pageNumber = pageNum;\n      this.getpagingStudent();\n    }\n  }\n};","map":{"version":3,"names":["axios","$","Paginate","useToast","data","isOpenPopupBonus","isOpenPopupPunish","pageDefault","pageNumber","txtSearch","totalPage","totalRecord","students","isShowDrop","dataSinhVienPunish","dataSinhVienBonus","components","watch","getpagingStudent","created","methods","SavePunish","me","toast","post","then","res","console","log","success","timeout","catch","error","SaveBonus","thongbao","alert","OpenPopupBonus","item","OpenPopupPunish","get","facultyID","classificationID","TotalPages","TotalRecords","Data","showPage","is","btnDropUp","toggleClass","addClass","removeClass","getPageDefault","e","isActive","target","getAttribute","clickCallback","pageNum"],"sources":["D:\\QLNS\\QLNS-DHTL\\src\\components\\page\\EducationManag.vue"],"sourcesContent":["<template>\r\n    <div class=\"body\">\r\n      <div class=\"popup-bonus\" v-if=\"isOpenPopupPunish\">\r\n        <div class=\"bonus-form\">\r\n          <div class=\"head-popup\">\r\n            <h3 class=\"title-heading\">Thêm mới thông tin Kỷ Luật</h3>\r\n            <div class=\"btn-close-popup\" @click.stop=\"isOpenPopupPunish = false\">\r\n              <div class=\"icon\">\r\n                <svg\r\n                  xmlns=\"http://www.w3.org/2000/svg\"\r\n                  fill=\"none\"\r\n                  viewBox=\"0 0 24 24\"\r\n                  stroke-width=\"1.5\"\r\n                  stroke=\"currentColor\"\r\n                  class=\"w-6 h-6\"\r\n                >\r\n                  <path\r\n                    stroke-linecap=\"round\"\r\n                    stroke-linejoin=\"round\"\r\n                    d=\"M9.75 9.75l4.5 4.5m0-4.5l-4.5 4.5M21 12a9 9 0 11-18 0 9 9 0 0118 0z\"\r\n                  />\r\n                </svg>\r\n              </div>\r\n            </div>\r\n          </div>\r\n  \r\n          <table>\r\n            <tr>\r\n              <td width=\"180px\">Khoa: <span class=\"color-red\">*</span></td>\r\n              <td>\r\n                {{ dataSinhVienPunish.FacultyName }}\r\n              </td>\r\n              <td style=\"width: 70px;\" width=\"10%\">Lớp: <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"1\">\r\n                {{dataSinhVienPunish.Class}}\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td >Sinh viên: <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"4\">\r\n                {{dataSinhVienPunish.StudentName}}\r\n              </td>\r\n              <!-- <td width=\"10%\">MSV <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"2\">\r\n                <input type=\"text\" v-model=\"dataSinhVienPunish.UserID\">\r\n              </td> -->\r\n            </tr>\r\n            <tr>\r\n              <td>Thêm hình thức kỷ luật <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"4\">\r\n                <input type=\"text\" v-model=\"dataSinhVienPunish.Form\">\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>Thêm nơi kỷ luật <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"4\">\r\n                <input type=\"text\" v-model=\"dataSinhVienPunish.Location\">\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>Chọn ngày kỷ luật <span class=\"color-red\">*</span></td>\r\n              <td><input type=\"date\" name=\"\" id=\"\" v-model=\"dataSinhVienPunish.DisciplineDate\"/></td>\r\n           \r\n            </tr>\r\n            <tr>\r\n              <td>Thêm lý do kỷ luật <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"4\">\r\n                <input type=\"text\" name=\"\" id=\"\" v-model=\"dataSinhVienPunish.Reason\"/>\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td class=\"btn-group\" colspan=\"5\">\r\n                <button @click.stop=\"SavePunish\" class=\"btn-action\">Lưu</button>\r\n                <button @click=\"thongbao\" class=\"btn-action\">Đóng</button>\r\n              </td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n      </div>\r\n  \r\n  \r\n  \r\n      <!-- end open popup ky luat   -->\r\n      <div class=\"popup-bonus\" v-if=\"isOpenPopupBonus\">\r\n        <div class=\"bonus-form\">\r\n          <div class=\"head-popup\">\r\n            <h3 class=\"title-heading\">Thêm mới thông tin khen thưởng</h3>\r\n            <div class=\"btn-close-popup\" @click.stop=\"isOpenPopupBonus = false\">\r\n              <div class=\"icon\">\r\n                <svg\r\n                  xmlns=\"http://www.w3.org/2000/svg\"\r\n                  fill=\"none\"\r\n                  viewBox=\"0 0 24 24\"\r\n                  stroke-width=\"1.5\"\r\n                  stroke=\"currentColor\"\r\n                  class=\"w-6 h-6\"\r\n                >\r\n                  <path\r\n                    stroke-linecap=\"round\"\r\n                    stroke-linejoin=\"round\"\r\n                    d=\"M9.75 9.75l4.5 4.5m0-4.5l-4.5 4.5M21 12a9 9 0 11-18 0 9 9 0 0118 0z\"\r\n                  />\r\n                </svg>\r\n              </div>\r\n            </div>\r\n          </div>\r\n  \r\n          <table>\r\n            <tr>\r\n              <td width=\"180px\">Khoa: <span class=\"color-red\">*</span></td>\r\n              <td  style=\"width: 200px\">\r\n                {{ dataSinhVienBonus.FacultyName }}\r\n              </td>\r\n              <td  width=\"70px\">Lớp: <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"1\">\r\n                {{dataSinhVienBonus.Class}}\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>Sinh viên <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"1\">\r\n                {{dataSinhVienBonus.StudentName}}\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>Hình thức khen thưởng <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"4\">\r\n                <input type=\"text\" v-model=\"dataSinhVienBonus.Form\">\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>Nơi khen thưởng <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"4\">\r\n                <input type=\"text\" v-model=\"dataSinhVienBonus.Location\">\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>Ngày khen thưởng <span class=\"color-red\">*</span></td>\r\n              <td><input type=\"date\" v-model=\"dataSinhVienBonus.BonusDate\" name=\"\" id=\"\" /></td>\r\n            \r\n            </tr>\r\n            <tr>\r\n              <td>Nội dung khen thưởng <span class=\"color-red\">*</span></td>\r\n              <td colspan=\"4\">\r\n                <input type=\"text\" v-model=\"dataSinhVienBonus.Reason\"  name=\"\" id=\"\" />\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td class=\"btn-group\" colspan=\"5\">\r\n                <button class=\"btn-action\" @click.stop=\"SaveBonus\">Lưu</button>\r\n                <button @click=\"isOpenPopupBonus = false\" class=\"btn-action\">Đóng</button>\r\n              </td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n      </div>\r\n      <h3 class=\"header-bd\">Đào tạo</h3>\r\n      <div class=\"body-header\">\r\n        <div class=\"row-input\">\r\n          <div class=\"input\">\r\n            <input type=\"text\" class=\"search\" placeholder=\"Tìm kiếm\"  v-model=\"txtSearch\"  @keypress.enter=\"getPagingEmployee\"  />\r\n            <div class=\"icon-search icon\"></div>\r\n          </div>\r\n         \r\n          \r\n        </div>\r\n     \r\n      </div>\r\n      <div id=\"m-table\" class=\"m-table\">\r\n        <table id=\"tbEmployee\" class=\"table\">\r\n          <thead>\r\n            <tr>\r\n              <th\r\n                class=\"sticky-left-top checkbox\"\r\n                colspan=\"1\"\r\n                style=\"min-width: 30px !important; text-align: center\"\r\n              >\r\n                <input\r\n                  ref=\"checkall\"\r\n                  type=\"checkBox\"\r\n                  class=\"check-all\"\r\n                  style=\"width: 18px; height: 18px\"\r\n                />\r\n              </th>\r\n              <th class=\"text-center\" style=\"min-width: 40px\">STT</th>\r\n              <th>Mã nhân viên </th>\r\n              <th>Họ và tên</th>\r\n              <th>Ngày sinh</th>\r\n              <th>Giới tính</th>\r\n              <th>Khoa</th>\r\n              <th>Lớp</th>\r\n              <th>Xếp loại</th>\r\n             \r\n              <th\r\n                class=\"th-item-final sticky-right-top\"\r\n                colspan=\"12\"\r\n                style=\"min-width: 100px; text-align: center\"\r\n              >\r\n                <div class=\"th-item\">\r\n                  <span class=\"table-text\">Thêm khen thưởng/kỉ luật</span>\r\n                </div>\r\n              </th>\r\n           \r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(item,i) of students\" :key=\"item.StudentID\" ref=\"row\">\r\n              <td\r\n                ref=\"rowCheck\"\r\n                class=\"checkbox sticky-left\"\r\n                style=\"text-align: center\"\r\n                colspan=\"1\"\r\n              >\r\n                <input\r\n                  ref=\"check\"\r\n                  class=\"check-item\"\r\n                  type=\"checkbox\"\r\n                  style=\"width: 18px; height: 18px\"\r\n                />\r\n              </td>\r\n              <td class=\"text-center\">{{ i+1 }}</td>\r\n              <td>{{ item.StudentCode }}</td>\r\n              <td>{{ item.StudentName }}</td>\r\n              <td>{{ item.DateOfBirth }}</td>\r\n              <td>{{ item.Gender }}</td>\r\n              <td>{{ item.FacultyName }}</td>\r\n              <td>{{ item.Class }}</td>\r\n              <td>{{ item.ClassifiOpenPopupBonuscationName }}</td>\r\n              \r\n              <td\r\n                ref=\"func\"\r\n                class=\"td-item-final td-func sticky-right\"\r\n                style=\"position: sticky; right: 0; background-color: #fff; width: auto;\"\r\n                colspan=\"12\"\r\n              >\r\n                <div class=\"edit-text\"></div>\r\n                <button class=\"bonus\"  @click=\"OpenPopupPunish(item)\">\r\n                 Kỉ luật\r\n                </button>\r\n                <button class=\"dis\" @click=\"OpenPopupBonus(item)\">\r\n                  Khen thưởng\r\n                </button>\r\n              </td> \r\n              \r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div class=\"paging\">\r\n        <div class=\"paging-left\">\r\n          Tổng số:\r\n          <strong>{{ totalRecord }}</strong>\r\n          bản ghi\r\n        </div>\r\n        <div class=\"paging-right\">\r\n          <div class=\"page\">\r\n            <div class=\"content-page\">{{ pageDefault }} bản ghi trên 1 trang</div>\r\n            <div class=\"dropup-page\">\r\n              <div class=\"icon-dropup\" @click=\"btnDropUp\"></div>\r\n              <div class=\"item-up\" v-show=\"isShowDrop\">\r\n                <div\r\n                  class=\"item-dropup\"\r\n                  :class=\"{ act: isActive == '10' }\"\r\n                  pageSize=\"10\"\r\n                  :value=\"pageDefault\"\r\n                  @click=\"getPageDefault\"\r\n                >\r\n                  10 bản ghi trên 1 trang\r\n                </div>\r\n                <div\r\n                  class=\"item-dropup\"\r\n                  :class=\"{ act: isActive == '20' }\"\r\n                  pageSize=\"20\"\r\n                  @click=\"getPageDefault\"\r\n                >\r\n                  20 bản ghi trên 1 trang\r\n                </div>\r\n                <div\r\n                  class=\"item-dropup\"\r\n                  :class=\"{ act: isActive == '30' }\"\r\n                  pageSize=\"30\"\r\n                  @click=\"getPageDefault\"\r\n                >\r\n                  30 bản ghi trên 1 trang\r\n                </div>\r\n                <div\r\n                  class=\"item-dropup\"\r\n                  :class=\"{ act: isActive == '50' }\"\r\n                  pageSize=\"50\"\r\n                  @click=\"getPageDefault\"\r\n                >\r\n                  50 bản ghi trên 1 trang\r\n                </div>\r\n                <div\r\n                  class=\"item-dropup\"\r\n                  :class=\"{ act: isActive == '100' }\"\r\n                  pageSize=\"100\"\r\n                  @click=\"getPageDefault\"\r\n                >\r\n                  100 bản ghi trên 1 trang\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div>\r\n            <Paginate\r\n              v-model=\"pageNumber\"\r\n              :page-count=\"totalPage\"\r\n              :page-range=\"3\"\r\n              :margin-pages=\"1\"\r\n              :click-handler=\"clickCallback\"\r\n              :prev-text=\"'Trước'\"\r\n              :next-text=\"'Sau'\"\r\n              :container-class=\"'pagination'\"\r\n              :page-class=\"'page-item'\"\r\n            >\r\n            </Paginate>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <Form v-show=\"isShow\" @hideForm=\"closeForm\"></Form>\r\n  </template>\r\n  <script>\r\n  import axios from \"axios\"\r\n  import $ from \"jquery\"\r\n  import Paginate from \"vuejs-paginate-next\";\r\n  import { useToast } from \"vue-toastification\";\r\n  export default {\r\n    data() {\r\n      return {\r\n        isOpenPopupBonus: false,\r\n        isOpenPopupPunish: false,\r\n        pageDefault:10,\r\n        pageNumber: 1,\r\n        txtSearch:\"\",\r\n        totalPage:1,\r\n        totalRecord:0,\r\n        students:{},\r\n        isShowDrop:false,\r\n        dataSinhVienPunish:[],\r\n        dataSinhVienBonus:{}\r\n\r\n      };\r\n    },\r\n    components: {\r\n      Paginate\r\n    },\r\n    watch: {\r\n    txtSearch: function () {\r\n      if (this.txtSearch == \"\") {\r\n        this.getpagingStudent();\r\n      }\r\n    },\r\n  },\r\n  crea