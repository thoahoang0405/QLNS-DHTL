{"ast":null,"code":"import Paginate from \"vuejs-paginate-next\";\nimport axios from \"axios\";\nimport Form from \"../base/FormEmployee.vue\";\nimport $ from \"jquery\";\nimport Combobox from \"../base/BaseCombobox2.vue\";\nimport popUp from \"../base/BasePopupDelete.vue\";\nexport default {\n  components: {\n    Paginate,\n    Form,\n    Combobox,\n    popUp\n  },\n  data() {\n    return {\n      isActive: \"20\",\n      pageNumber: 1,\n      page: 1,\n      employees: {},\n      totalPage: 1,\n      isShow: false,\n      //gán v-show=isShow hoặc v-show =false để ẩn form\n      isShowDrop: false,\n      totalRecord: 0,\n      pageDefault: 20,\n      department: {},\n      position: {},\n      departmentID: \"\",\n      positionID: \"\",\n      txtSearch: \"\",\n      employeeSelect: {},\n      newCode: \"\",\n      formMode: 1,\n      employeeID: \"\",\n      isShowPopup: false,\n      empCodeDelete: \"\",\n      empID: \"\"\n    };\n  },\n  created() {\n    this.getPagingEmployee();\n    this.getDepartment();\n    this.getPosition();\n  },\n  watch: {\n    txtSearch: function () {\n      if (this.txtSearch == \"\") {\n        this.getPagingEmployee();\n      }\n    }\n  },\n  methods: {\n    deleteEmployee(emp) {\n      this.isShowPopup = !this.isShow;\n      this.empCodeDelete = emp.EmployeeCode;\n      this.empID = emp.EmployeeID;\n    },\n    delete(value) {\n      this.isShowPopup = value;\n      var me = this;\n      try {\n        axios.delete(`https://localhost:44301/api/Employees/${me.empID}`).then(function (res) {\n          console.log(res);\n          alert(\"xóa ok\");\n          me.getPagingEmployee;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    editEmployee(emp) {\n      this.employeeSelect = emp;\n      this.employeeID = emp.EmployeeID;\n      this.isShow = true;\n      this.formMode = 2;\n    },\n    getPagingEmployee() {\n      try {\n        var me = this;\n        axios.get(`https://localhost:44301/api/Employees/Filter?keyword=${this.txtSearch}&pageSize=${this.pageDefault}&departmentID=${this.departmentID}&positionID=${this.positionID}&pageNumber=${this.pageNumber}`).then(function (res) {\n          me.totalPage = res.data.TotalPages;\n          me.totalRecord = res.data.TotalRecords;\n          me.employees = res.data.Data;\n          console.log(me.totalPage);\n          console.log(me.employees);\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    getDepartment() {\n      try {\n        var me = this;\n        axios.get(\"https://localhost:44301/api/Departments\").then(function (res) {\n          me.department = res.data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    getPosition() {\n      try {\n        var me = this;\n        axios.get(\"https://localhost:44301/api/Position\").then(function (res) {\n          me.position = res.data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    formatDate(date) {\n      try {\n        if (date) {\n          date = new Date(date);\n          let newDate = date.getDate();\n          let month = date.getMonth() + 1;\n          let year = date.getFullYear();\n          newDate = newDate < 10 ? `0${newDate}` : newDate;\n          month = month < 10 ? `0${month}` : month;\n          return `${newDate}-${month}-${year}`;\n        }\n      } catch (error) {\n        return \"\";\n      }\n    },\n    selectItemCbb(value) {\n      if (value.DepartmentID) {\n        this.departmentID = value.DepartmentID;\n      } else {\n        this.departmentID = \"\";\n        this.getPagingEmployee();\n      }\n      this.getPagingEmployee();\n    },\n    selectItemCategory(value) {\n      console.log(value.PositionsID);\n      if (value.PositionsID) {\n        this.positionID = value.PositionsID;\n      } else {\n        this.positionID = \"\";\n        this.getPagingEmployee();\n      }\n      this.getPagingEmployee();\n    },\n    showPage(is) {\n      this.isShowDrop = is;\n    },\n    btnShow() {\n      this.employeeSelect = {};\n      this.getNewCode();\n      this.formMode = 1;\n      this.isShow = !this.isShow;\n    },\n    getNewCode() {\n      try {\n        var me = this;\n        axios.get(\"https://localhost:44301/api/Employees/NewCode\").then(function (res) {\n          me.newCode = res.data;\n        }).catch(function () {\n          console.log(1);\n        });\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    //nhận lệnh ẩn từ bên form chi tiết\n    closeForm(value) {\n      this.isShow = value;\n    },\n    btnDropUp() {\n      $(\".icon-dropup\").toggleClass(\"iconrotate\");\n      if (!this.isShowDrop) {\n        this.showPage(true);\n        $(\".icon-dropup\").addClass(\"iconrotate\");\n      } else {\n        this.showPage(false);\n        $(\".icon-dropup\").removeClass(\"iconrotate\");\n      }\n    },\n    gender(gender) {\n      switch (gender) {\n        case 0:\n          gender = \"Nam\";\n          break;\n        case 1:\n          gender = \"Nữ\";\n          break;\n        default:\n          break;\n      }\n      return gender;\n    },\n    getPageDefault(e) {\n      this.isActive = e.target.getAttribute(\"pageSize\");\n      this.pageDefault = e.target.getAttribute(\"pageSize\");\n      this.showPage(false);\n      $(\".icon-dropup\").removeClass(\"iconrotate\");\n      this.getPagingEmployee();\n      if (this.pageDefault > this.totalRecord) {\n        this.pageDefault = this.totalRecord;\n      }\n    },\n    clickCallback(pageNum) {\n      this.pageNumber = pageNum;\n      this.getPagingEmployee();\n    }\n  }\n};","map":{"version":3,"names":["Paginate","axios","Form","$","Combobox","popUp","components","data","isActive","pageNumber","page","employees","totalPage","isShow","isShowDrop","totalRecord","pageDefault","department","position","departmentID","positionID","txtSearch","employeeSelect","newCode","formMode","employeeID","isShowPopup","empCodeDelete","empID","created","getPagingEmployee","getDepartment","getPosition","watch","methods","deleteEmployee","emp","EmployeeCode","EmployeeID","delete","value","me","then","res","console","log","alert","catch","error","editEmployee","get","TotalPages","TotalRecords","Data","formatDate","date","Date","newDate","getDate","month","getMonth","year","getFullYear","selectItemCbb","DepartmentID","selectItemCategory","PositionsID","showPage","is","btnShow","getNewCode","closeForm","btnDropUp","toggleClass","addClass","removeClass","gender","getPageDefault","e","target","getAttribute","clickCallback","pageNum"],"sources":["D:\\dhtl\\ptda\\UI\\QLNS-DHTL\\src\\components\\page\\EmployeeManag.vue"],"sourcesContent":["<template>\r\n  <div class=\"body\">\r\n    <h3 class=\"header-bd\">Quản lý sinh viên</h3>\r\n    <div class=\"body-header\">\r\n      <div class=\"row-input\">\r\n        <div class=\"input\">\r\n          <input type=\"text\" class=\"search\"   v-model=\"txtSearch\"  @keypress.enter=\"getPagingEmployee\"  placeholder=\"Tìm kiếm sinh viên\" />\r\n          <div class=\"icon-search icon\"></div>\r\n        </div>\r\n        \r\n        <Combobox\r\n          :items=\"department\"\r\n         \r\n          :fieldName=\"'DepartmentName'\"\r\n         \r\n          @selectedItem=\"selectItemCbb\"\r\n        ></Combobox>\r\n      \r\n        <Combobox\r\n          :items=\"position\"       \r\n          :fieldName=\"'PositionsName'\"\r\n          @selectedItem=\"selectItemCategory\"\r\n         \r\n        ></Combobox>\r\n      </div>\r\n      <div class=\"button-function\">\r\n        <div class=\"add\">\r\n          <button @click=\"btnShow\" class=\"btn-add btn-hover-blue\">+ Thêm nhân viên</button>\r\n        </div>\r\n\r\n        <button @click=\"getPagingEmployee\" class=\"btn-excel\">\r\n          <div class=\"icon icon-load\">\r\n            <div class=\"tooltip-excel\">Lấy lại dữ liệu</div>\r\n          </div>\r\n        </button>\r\n        \r\n      </div>\r\n    </div>\r\n    <div id=\"m-table\" class=\"m-table\">\r\n      <table id=\"tbEmployee\" class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th\r\n              class=\"sticky-left-top checkbox\"\r\n              colspan=\"1\"\r\n              style=\"min-width: 30px !important; text-align: center\"\r\n            >\r\n              <input\r\n                ref=\"checkall\"\r\n                type=\"checkBox\"\r\n                class=\"check-all\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </th>\r\n            <th class=\"text-center\" style=\"min-width: 40px;\">STT</th>\r\n            <th>Mã nhân viên</th>\r\n              <th>Họ và tên</th>\r\n              <th>Giới tính</th>\r\n              <th>Ngày sinh</th>\r\n              <th>Số CMND/CCCD</th>\r\n              <th>Email</th>\r\n              <th>Số điện thoại</th>\r\n              <th>Mã số thuế</th>\r\n              <th>Số tài khoản</th>\r\n              <th>Tên ngân hàng</th>\r\n              <th>Địa chỉ</th>\r\n              <th>Khoa</th>\r\n           \r\n              <th>Trạng thái</th>\r\n              <th>Cấp bậc lương</th>\r\n              <th>Phòng ban</th>\r\n              <th>Chứng chỉ đào tạo</th>\r\n              <th>Tình trạng hôn nhân</th>\r\n              <th>Vị trí</th>\r\n              \r\n            <th\r\n              class=\"th-item-final sticky-right-top\"\r\n              colspan=\"12\"\r\n              style=\"min-width: 100px; text-align: center\"\r\n            >\r\n              <div class=\"th-item\">\r\n                <span class=\"table-text\">Chức năng </span>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr ref=\"row\" v-for=\"(emp, index) of employees\" :key=\"emp.EmployeeCode\">\r\n            <td\r\n              ref=\"rowCheck\"\r\n              class=\"checkbox sticky-left\"\r\n              style=\"text-align: center\"\r\n              colspan=\"1\"\r\n            >\r\n              <input\r\n                ref=\"check\"\r\n                class=\"check-item\"\r\n                type=\"checkbox\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </td>\r\n            <td class=\"text-center\">{{ index+1 }}</td>\r\n            <td>{{ emp.EmployeeCode }}</td>\r\n            <td>{{ emp.EmployeeName }}</td>\r\n            <td>{{ gender(emp.Gender) }}</td>\r\n            <td>{{ formatDate(emp.DateOfBirth) }}</td>\r\n            <td>{{ emp.IdentityNumber }}</td>\r\n            <td>{{ emp.Email }}</td>\r\n            <td>{{ emp.Phonenumber }}</td>\r\n            <td>{{ emp.TaxCode }}</td>\r\n            <td>{{ emp.BankAccountNumber }}</td>\r\n            <td>{{ emp.BankAccountName }}</td>\r\n            \r\n            <td>{{ emp.Adress }}</td>\r\n            <td>{{ emp.FacultyName }}</td>\r\n            <td>{{ emp.StatusEmployeeName }}</td>\r\n            <td>{{ emp.SalaryName }}</td>\r\n            <td>{{ emp.DepartmentName }}</td>\r\n            <td>{{ emp.TrainingCertificateName}}</td>\r\n            <td>{{ emp.MaritalStatus }}</td>\r\n            <td>{{ emp.PositionsName }}</td>\r\n           \r\n            <td\r\n              ref=\"func\"\r\n              class=\"td-item-final td-func sticky-right\"\r\n              style=\"position: sticky; right: 0; background-color: #fff\"\r\n              colspan=\"12\"\r\n            >\r\n              <div class=\"edit-text\"></div>\r\n              <div class=\"icon icon-edit\" @click=\"editEmployee(emp)\" ></div>\r\n                <div class=\"icon icon-delete\" @click=\"deleteEmployee(emp)\"></div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <div class=\"paging\">\r\n      <div class=\"paging-left\">\r\n        Tổng số:\r\n        <strong>{{ totalRecord }}</strong>\r\n        bản ghi\r\n      </div>\r\n      <div class=\"paging-right\">\r\n        <div class=\"page\">\r\n          <div class=\"content-page\">{{ pageDefault }} bản ghi trên 1 trang</div>\r\n          <div class=\"dropup-page\">\r\n            <div class=\"icon-dropup\" @click=\"btnDropUp\"></div>\r\n            <div class=\"item-up\" v-show=\"isShowDrop\">\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '10' }\"\r\n                pageSize=\"10\"\r\n                \r\n                @click=\"getPageDefault\"\r\n              >\r\n                10 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '20' }\"\r\n                pageSize=\"20\"\r\n                @click=\"getPageDefault\"\r\n                :value=\"pageDefault\"\r\n              >\r\n                20 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '30' }\"\r\n                pageSize=\"30\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                30 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '50' }\"\r\n                pageSize=\"50\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                50 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '100' }\"\r\n                pageSize=\"100\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                100 bản ghi trên 1 trang\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <Paginate\r\n          v-model=\"page\"\r\n                    :page-count=\"totalPage\"\r\n                    :page-range=\"2\"\r\n                    :margin-pages=\"1\"\r\n                    :click-handler=\"clickCallback\"\r\n                    :prev-link-class=\"'page-link'\"\r\n                    :prev-text=\"'Trước'\"\r\n                    :prev-class=\"'page-item'\"\r\n                    :next-link-class=\"'page-link'\"\r\n                    :next-text=\"'Sau'\"\r\n                    :next-class=\"'page-item'\"\r\n                    :container-class=\"'pagination'\"\r\n                    :page-class=\"'page-item'\"\r\n          >\r\n          </Paginate>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <popUp v-show=\"isShowPopup\" :msv=\"empCodeDelete\" @closeNotifi=\"delete\"></popUp>\r\n<Form v-show=\"isShow\" @hideForm=\"closeForm\" :loadData=\"getPagingEmployee\" :employeeId=\"employeeID\"  :FormMode=\"formMode\" :employeeSL=\"employeeSelect\" :code=\"newCode\"></Form>\r\n</template>\r\n<style>\r\n  .btn-add:hover {\r\n    opacity: 0.8;\r\n  }\r\n  .border-red {\r\n  border: 1px solid red;\r\n  }\r\n \r\n</style>\r\n<script>\r\nimport Paginate from \"vuejs-paginate-next\";\r\nimport axios from \"axios\";\r\nimport Form from \"../base/FormEmployee.vue\"\r\nimport $ from \"jquery\";\r\nimport Combobox from \"../base/BaseCombobox2.vue\";\r\nimport popUp from \"../base/BasePopupDelete.vue\"\r\nexport default {\r\n  components: {\r\n    Paginate,Form,Combobox,popUp\r\n  },\r\n  data() {\r\n    return {\r\n      isActive: \"20\",\r\n      pageNumber: 1,\r\n      page: 1,\r\n      employees:{},\r\n      totalPage: 1,\r\n      isShow: false, //gán v-show=isShow hoặc v-show =false để ẩn form\r\n      isShowDrop: false,\r\n      totalRecord: 0,\r\n      pageDefault: 20,\r\n      department:{},\r\n      position:{},\r\n      departmentID:\"\",\r\n      positionID:\"\",\r\n      txtSearch:\"\",\r\n      employeeSelect:{},\r\n      newCode: \"\",\r\n      formMode: 1,\r\n      employeeID:\"\",\r\n      isShowPopup:false,\r\n      empCodeDelete:\"\",\r\n      empID:\"\"\r\n    };\r\n  },\r\n  created() {\r\n    this.getPagingEmployee()\r\n    this.getDepartment()\r\n    this.getPosition()\r\n  },\r\n  watch: {\r\n    txtSearch: function () {\r\n      if (this.txtSearch == \"\") {\r\n        this.getPagingEmployee();\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    deleteEmployee(emp){\r\n      this.isShowPopup=!this.isShow\r\n      this.empCodeDelete= emp.EmployeeCode\r\n      this.empID=emp.EmployeeID\r\n    },\r\n    delete(value){\r\n      this.isShowPopup=value\r\n      var me = this;\r\n      try{\r\n       axios\r\n         .delete(\r\n          `https://localhost:44301/api/Employees/${me.empID}`\r\n         )\r\n         .then(function (res) {\r\n          console.log(res);\r\n          alert(\"xóa ok\")\r\n          me.getPagingEmployee\r\n         })\r\n        \r\n         .catch(function () {\r\n           console.log(1);\r\n         });\r\n     } catch (error) {\r\n       console.log(error);\r\n     }\r\n    },\r\n    editEmployee(emp){\r\n      this.employeeSelect=emp\r\n      this.employeeID=emp.EmployeeID\r\n     this.isShow=true\r\n     this.formMode=2\r\n    },\r\n    getPagingEmployee() {\r\n      try {\r\n       \r\n        var me = this;\r\n       \r\n        axios\r\n          .get(\r\n            `https://localhost:44301/api/Employees/Filter?keyword=${this.txtSearch}&pageSize=${this.pageDefault}&departmentID=${this.departmentID}&positionID=${this.positionID}&pageNumber=${this.pageNumber}`\r\n          )\r\n          .then(function (res) {\r\n          \r\n            me.totalPage = res.data.TotalPages;\r\n            me.totalRecord = res.data.TotalRecords;\r\n            me.employees = res.data.Data;\r\n            console.log( me.totalPage);\r\n            c