s.getPosition();\r\n    this.getAllowance();\r\n    this.getSNL();\r\n    // this.getCerti();\r\n  },\r\n  methods: {\r\n    save(){\r\n     \r\n        // // this.getAllEmployee()\r\n        // this.validateDateOfBirth()\r\n        // this.validateEmail()\r\n        // this.validateEmployeeCode()\r\n        // this.validateIdentity()\r\n\r\n        // this.validateName()\r\n        // if(this.isValid==true){\r\n          if(this.formMode==0){\r\n          this.addSalary()\r\n       \r\n          }\r\n      else{\r\n        this.editEmployee()\r\n      }\r\n    \r\n\r\n    },\r\n    closePoup(value){\r\nthis.isShowPop=value\r\n    },\r\n    getNewCode(){\r\n      try {\r\n       \r\n       var me = this;\r\n      \r\n       axios\r\n         .get(\r\n          \"https://localhost:44301/api/Employees/NewCode\"\r\n         )\r\n         .then(function (res) {\r\n          me.employee.EmployeeCode=res.data\r\n         })\r\n        \r\n         .catch(function () {\r\n           console.log(1);\r\n         });\r\n     } catch (error) {\r\n       console.log(error);\r\n     }\r\n    },\r\n    addSalary(){\r\n        var me = this;\r\n      console.log(me.salary);\r\n       \r\n      const toast = useToast();\r\n      \r\n        try {\r\n        axios\r\n          .post(\"https://localhost:44301/api/luong\", me.salary)\r\n          .then(function (res) {\r\n            console.log(\"ok\", res.data);\r\n            me.salary={}\r\n           \r\n            toast.success(\"thêm dữ liệu thành công\", { timeout: 2000 });\r\n            me.$emit()\r\n            // me.loadData()\r\n          })\r\n         \r\n          .catch(function () {\r\n            toast.error(\"thêm dữ liệu thất bại\", { timeout: 2000 });\r\n          });\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n      \r\n     \r\n    },\r\n    editEmployee(){\r\n      var me = this;\r\n        console.log(me.employee);\r\n        me.employee.Gender=parseInt( me.employee.Gender)\r\n   