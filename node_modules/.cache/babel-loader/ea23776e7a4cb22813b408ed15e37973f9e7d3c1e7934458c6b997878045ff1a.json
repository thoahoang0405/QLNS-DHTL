);\n          let month = date.getMonth() + 1;\n          let year = date.getFullYear();\n          newDate = newDate < 10 ? `0${newDate}` : newDate;\n          month = month < 10 ? `0${month}` : month;\n          return `${newDate}-${month}-${year}`;\n        }\n      } catch (error) {\n        return \"\";\n      }\n    },\n    selectItemCbb(value) {\n      if (value.FacultyID) {\n        this.FacultyID = value.FacultyID;\n      } else {\n        this.facultyID = \"\";\n        this.getPagingEmployee();\n      }\n      this.getPagingEmployee();\n    },\n    selectItemCategory(value) {\n      console.log(value.PositionsID);\n      if (value.PositionsID) {\n        this.positionID = value.PositionsID;\n      } else {\n        this.positionID = \"\";\n        this.getPagingEmployee();\n      }\n      this.getPagingEmployee();\n    },\n    clickAdd(item) {\n      this.list.push(item);\n    },\n    showPage(is) {\n      this.isShowDrop = is;\n    },\n    btnAddStudent() {\n      this.isShowForm = !this.isShowForm;\n    },\n    btnEditStudent(item) {\n      this.isShowForm = !this.isShowForm;\n      this.studentSelected = item;\n    },\n    deleteClick(item) {\n      this.isShowConfirm = !this.isShowConfirm;\n      console.log(item.msv);\n      this.msvDelete = item.msv;\n    },\n    cancelConfirm(value) {\n      this.isShowConfirm = value;\n    },\n    //nhận lệnh ẩn từ bên form chi tiết\n    closeFormDetail(value) {\n      console.log(value);\n      this.isShow = value;\n    },\n    hideFormDetail(value) {\n      this.isShowForm = value;\n    },\n    btnDropUp() {\n      $(\".icon-dropup\").toggleClass(\"iconrotate\");\n      if (!this.isShowDrop) {\n        this.showPage(true);\n        $(\".icon-dropup\").addClass(\"iconrotate\");\n      } else {\n        this.showPage(false);\n        $(\".icon-dropup\").removeClass(\"iconrotate\");\n      }\n    },\n    getPageDefault(e) {\n      this.isActive = e.target.getAttribute(\"pageSize\");\n      this.pageDefault = e.target.getAttribute(\"pageSize\");\n      this.showPage(false);\n      $(\".icon-dropup\").removeClass(\"iconrotate\");\n      this.filterEmployee();\n      if (this.pageDefault > this.totalRecord) {\n        this.pageDefault = this.totalRecord;\n      }\n    },\n    clickCallback(pageNum) {\n      this.pageNumber = pageNum;\n      this.filterEmployee();\n    }\n  }\n};","map":{"version":3,"names":["Paginate","Form","PopupConfirm","$","Combobox","components","data","list","id","msv","ten","gioitinh","ngaysinh","cmnd","email","sodt","sotaikhoan","diachi","khoa","kyhoc","khenthuong","kyluat","hocbong","trangthai","tendangnhap","matkhau","isActive","studentSelected","pageNumber","page","totalPage","isShowConfirm","isShowForm","isShow","isShowDrop","students","pageDefault","msvDelete","faculty","facultyID","classification","classificationID","txtSearch","watch","getPagingEmployee","created","getDepartment","getPosition","methods","me","axios","get","then","res","TotalPages","totalRecord","TotalRecords","Data","catch","console","log","error","formatDate","date","Date","newDate","getDate","month","getMonth","year","getFullYear","selectItemCbb","value","FacultyID","selectItemCategory","PositionsID","positionID","clickAdd","item","push","showPage","is","btnAddStudent","btnEditStudent","deleteClick","cancelConfirm","closeFormDetail","hideFormDetail","btnDropUp","toggleClass","addClass","removeClass","getPageDefault","e","target","getAttribute","filterEmployee","clickCallback","pageNum"],"sources":["D:\\dhtl\\ptda\\UI\\QLNS-DHTL\\src\\components\\page\\StudentManag.vue"],"sourcesContent":["<template>\r\n  <div class=\"body\">\r\n    <h3 class=\"header-bd\">Quản lý sinh viên</h3>\r\n    <div class=\"body-header\">\r\n      <div class=\"row-input\">\r\n        <div class=\"input\">\r\n          <input type=\"text\" class=\"search\" placeholder=\"Tìm kiếm sinh viên\" />\r\n          <div class=\"icon-search icon\"></div>\r\n        </div>\r\n        <Combobox\r\n          :items=\"faculty\"\r\n         \r\n          :fieldName=\"'FacultyID'\"\r\n         \r\n          @selectedItem=\"selectItemCbb\"\r\n        ></Combobox>\r\n      \r\n        <Combobox\r\n          :items=\"classification\"       \r\n          :fieldName=\"'ClassificationID'\"\r\n          @selectedItem=\"selectItemCategory\"\r\n         \r\n        ></Combobox>\r\n      </div>\r\n      <div class=\"button-function\">\r\n        <div class=\"add\">\r\n          <button @click=\"btnAddStudent\" class=\"btn-add btn-hover-blue\">+ Thêm sinh viên</button>\r\n        </div>\r\n\r\n        <button class=\"btn-excel\">\r\n          <div class=\"icon icon-excel\">\r\n            <div class=\"tooltip-excel\">Xuất ra excel</div>\r\n          </div>\r\n        </button>\r\n        \r\n      </div>\r\n    </div>\r\n    <div id=\"m-table\" class=\"m-table\">\r\n      <table id=\"tbEmployee\" class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th\r\n              class=\"sticky-left-top checkbox\"\r\n              colspan=\"1\"\r\n              style=\"min-width: 30px !important; text-align: center\"\r\n            >\r\n              <input\r\n                ref=\"checkall\"\r\n                type=\"checkBox\"\r\n                class=\"check-all\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </th>\r\n            <th class=\"text-center\" style=\"min-width: 40px;\">STT</th>\r\n            <th>Mã sinh viên</th>\r\n            <th>Họ và tên</th>\r\n            <th>Giới tính</th>\r\n            <th>Ngày sinh</th>\r\n            <th>Số CMND/CCCD</th>\r\n            <th>Email</th>\r\n            <th>Số điện thoại</th>\r\n            <th>Số tài khoản</th>\r\n            <th>Địa chỉ</th>\r\n            <th>Khoa</th>\r\n            <th>Kì học</th>\r\n            <th>Khen thưởng</th>\r\n            <th>Kỉ luật</th>\r\n            <th>Học bổng</th>\r\n            <th>Trạng thái</th>\r\n            <th>Tên đăng nhập</th>\r\n            <th>mật khẩu</th>\r\n            <th\r\n              class=\"th-item-final sticky-right-top\"\r\n              colspan=\"12\"\r\n              style=\"min-width: 100px; text-align: center\"\r\n            >\r\n              <div class=\"th-item\">\r\n                <span class=\"table-text\">Chức năng </span>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr ref=\"row\" v-for=\"item in list\" :key=\"item.id\" @dblclick=\"btnEditStudent(item)\" >\r\n            <td\r\n              ref=\"rowCheck\"\r\n              class=\"checkbox sticky-left\"\r\n              style=\"text-align: center\"\r\n              colspan=\"1\"\r\n            >\r\n              <input\r\n                ref=\"check\"\r\n                class=\"check-item\"\r\n                type=\"checkbox\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </td>\r\n            <td class=\"text-center\">{{ item.id }}</td>\r\n            <td>{{ item.msv }}</td>\r\n            <td>{{ item.ten }}</td>\r\n            <td>{{ item.gioitinh }}</td>\r\n            <td>{{ item.ngaysinh }}</td>\r\n             <td>{{ item.cmnd }}</td>\r\n            <td>{{ item.email }}</td>\r\n            <td>{{ item.sodt }}</td>\r\n            <td>{{ item.sotaikhoan }}</td>\r\n            <td>{{ item.diachi }}</td>\r\n            <td>{{ item.khoa }}</td>\r\n            <td>{{ item.kyhoc }}</td>\r\n            <td>{{ item.khenthuong }}</td>\r\n            <td>{{ item.kyluat }}</td>\r\n            <td>{{ item.hocbong }}</td>\r\n            <td>{{ item.trangthai }}</td>\r\n            <td>{{ item.tendangnhap }}</td>\r\n            <td>{{ item.matkhau }}</td>\r\n            <td\r\n              ref=\"func\"\r\n              class=\"td-item-final td-func sticky-right\"\r\n              style=\"position: sticky; right: 0; background-color: #fff\"\r\n              colspan=\"12\"\r\n            >\r\n              <div class=\"ed