    >\r\n              <input\r\n                ref=\"checkall\"\r\n                type=\"checkBox\"\r\n                class=\"check-all\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </th>\r\n            <th class=\"text-center\" style=\"min-width: 40px;\">STT</th>\r\n              <th>Mã nhân viên</th>\r\n              <th>Họ và tên</th>\r\n              <th>Ngày sinh</th>\r\n              <th>Giới tính</th>\r\n              <th>Tôn giáo</th>\r\n              <th>Dân tộc</th>\r\n              <th>CCCD</th>\r\n              <th>Ngày Cấp</th>\r\n              <th>Nơi Cấp</th>\r\n              <th>Email </th>\r\n              <th>Địa chỉ</th>\r\n              <th>SĐT</th>\r\n              <th>Tên Khoa</th>\r\n              <!-- <th>Sơ yếu lý lịch</th> -->\r\n              <th>Trạng thái</th>\r\n             \r\n            <th\r\n              class=\"th-item-final sticky-right-top\"\r\n              colspan=\"12\"\r\n              style=\"min-width: 100px; text-align: center\"\r\n            >\r\n              <div class=\"th-item\">\r\n                <span class=\"table-text\">Chức năng </span>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody >\r\n          <tr ref=\"row\" v-for=\"(emp, index) of employees\" :key=\"emp.IDNhanVien\">\r\n            <td\r\n              ref=\"rowCheck\"\r\n              class=\"checkbox sticky-left\"\r\n              style=\"text-align: center\"\r\n              colspan=\"1\"\r\n            >\r\n              <input\r\n                ref=\"check\"\r\n                class=\"check-item\"\r\n                type=\"checkbox\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </td>\r\n            <td class=\"text-center\">{{ index+1 }}</td>\r\n            <td>{{ emp.MaNV }}</td>\r\n            <td>{{ emp.TenNV }}</td>\r\n            <td>{{ formatDate(emp.NgaySinh) }}</td>\r\n            <td>{{ gender(emp.GioiTinh) }}</td>\r\n            <td>{{  emp.TonGiao }}</td>\r\n            <td>{{  emp.DanToc }}</td>\r\n            <td>{{  emp.CCCD }}</td>\r\n            <td>{{  formatDate(emp.NgayCap) }}</td>\r\n            <td>{{  emp.NoiCap }}</td>\r\n            <td>{{ emp.Email }}</td>\r\n            <td>{{ emp.DiaChi }}</td>\r\n            <td>{{ emp.SDT }}</td>\r\n            <td>{{ emp.TenKhoa }}</td>\r\n            <!-- <td>\r\n                <div class=\"desc\" @click=\"OpenPopupCurriculumVitae(emp)\">\r\n                  Xem chi tiết\r\n                </div>\r\n            </td> -->\r\n            <td>\r\n                <div class=\"desc\" @click=\"OpenPopupState(emp)\">\r\n                  Xem chi tiết\r\n                </div>\r\n            </td> \r\n            <td\r\n              ref=\"func\"\r\n              class=\"td-item-final td-func sticky-right; align-items: center;\"\r\n              style=\"position: sticky; right: 0; background-color: #fff\"\r\n              colspan=\"12\"\r\n            >\r\n              <div class=\"edit-text\"></div> \r\n              <div class=\"icon icon-edit\" @click=\"OpenPopupFormEditCurriculumVitae(emp)\" ></div>\r\n              <div class=\"icon icon-delete\" @click=\"deleteEmployee(emp)\"></div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <div class=\"paging\">\r\n      <div class=\"paging-left\">\r\n        Tổng số:\r\n        <strong>{{ totalRecord }}</strong>\r\n        bản ghi\r\n      </div>\r\n      <div class=\"paging-right\">\r\n        <div class=\"page\">\r\n          <div class=\"content-page\">{{ pageDefault }} bản ghi trên 1 trang</div>\r\n          <div class=\"dropup-page\">\r\n            <div class=\"icon-dropup\" @click=\"btnDropUp\"></div>\r\n            <div class=\"item-up\" v-show=\"isShowDrop\">\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '10' }\"\r\n                pageSize=\"10\"\r\n                \r\n                @click=\"getPageDefault\"\r\n              >\r\n                10 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '20' }\"\r\n                pageSize=\"20\"\r\n                @click=\"getPageDefault\"\r\n                :value=\"pageDefault\"\r\n              >\r\n                20 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '30' }\"\r\n                pageSize=\"30\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                30 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '50' }\"\r\n                pageSize=\"50\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                50 bản ghi trên 1 trang\r\n              </div>\r\n              <div\r\n                class=\"item-dropup\"\r\n                :class=\"{ act: isActive == '100' }\"\r\n                pageSize=\"100\"\r\n                @click=\"getPageDefault\"\r\n              >\r\n                100 bản ghi trên 1 trang\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <Paginate\r\n          v-model=\"page\"\r\n                    :page-count=\"totalPage\"\r\n                    :page-range=\"2\"\r\n                    :margin-pages=\"1\"\r\n                    :click-handler=\"clickCallback\"\r\n                    :prev-link-class=\"'page-link'\"\r\n                    :prev-text=\"'Trước'\"\r\n                    :prev-class=\"'page-item'\"\r\n                    :next-link-class=\"'page-link'\"\r\n                    :next-text=\"'Sau'\"\r\n                    :next-class=\"'page-item'\"\r\n                    :c