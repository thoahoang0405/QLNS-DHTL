 <div class=\"body-header\">\r\n      <div class=\"row-input\">\r\n        <div class=\"input\">\r\n          <input type=\"text\" class=\"search\" placeholder=\"Tìm kiếm sinh viên\" />\r\n          <div class=\"icon-search icon\"></div>\r\n        </div>\r\n        <Combobox\r\n          :items=\"faculty\"\r\n         \r\n          :fieldName=\"'FacultyName'\"\r\n         \r\n          @selectedItem=\"selectItemCbb\"\r\n        ></Combobox>\r\n      \r\n        <Combobox\r\n          :items=\"classification\"       \r\n          :fieldName=\"'ClassificationName'\"\r\n          @selectedItem=\"selectItemCategory\"\r\n         \r\n        ></Combobox>\r\n      </div>\r\n      <div class=\"button-function\">\r\n        <div class=\"add\">\r\n          <button @click=\"btnAddStudent\" class=\"btn-add btn-hover-blue\">+ Thêm sinh viên</button>\r\n        </div>\r\n\r\n        <button class=\"btn-excel\">\r\n          <div class=\"icon icon-excel\">\r\n            <div class=\"tooltip-excel\">Xuất ra excel</div>\r\n          </div>\r\n        </button>\r\n        \r\n      </div>\r\n    </div>\r\n    <div id=\"m-table\" class=\"m-table\">\r\n      <table id=\"tbEmployee\" class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th\r\n              class=\"sticky-left-top checkbox\"\r\n              colspan=\"1\"\r\n              style=\"min-width: 30px !important; text-align: center\"\r\n            >\r\n              <input\r\n                ref=\"checkall\"\r\n                type=\"checkBox\"\r\n                class=\"check-all\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </th>\r\n            <th class=\"text-center\" style=\"min-width: 40px;\">STT</th>\r\n            <th>Mã sinh viên</th>\r\n            <th>Họ và tên</th>\r\n            <th>Giới tính</th>\r\n            <th>Ngày sinh</th>\r\n            <th>Số CMND/CCCD</th>\r\n            <th>Email</th>\r\n            <th>Số điện thoại</th>\r\n            <th>Số tài khoản</th>\r\n            <th>Tên ngân hàng</th>\r\n            <th>Địa chỉ</th>\r\n            <th>Khoa</th>\r\n            <th>Lớp</th>\r\n            <th>Chương trình đào tạo</th>\r\n            <th>Xếp loại</th>\r\n            <th>Trạng thái</th>\r\n           \r\n          \r\n            <th\r\n              class=\"th-item-final sticky-right-top\"\r\n              colspan=\"12\"\r\n              style=\"min-width: 100px; text-align: center\"\r\n            >\r\n              <div class=\"th-item\">\r\n                <span class=\"table-text\">Chức năng </span>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr ref=\"row\" v-for=\"(item,index) in students\" :key=\"item.StudentID\" @dblclick=\"btnEditStudent(item)\" >\r\n            <td\r\n              ref=\"rowCheck\"\r\n              class=\"checkbox sticky-left\"\r\n              style=\"text-align: center\"\r\n              colspan=\"1\"\r\n            >\r\n              <input\r\n                ref=\"check\"\r\n                class=\"check-item\"\r\n                type=\"checkbox\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </td>\r\n            <td class=\"text-center\">{{ index+1 }}</td>\r\n            <td>{{ item.StudentCode }}</td>\r\n            <td>{{ item.StudentName }}</td>\r\n            <td>{{ gender(item.Gender) }}</td>\r\n            <td>{{ formatDate(item.DateOfBirth) }}</td>\r\n             <td>{{ item.IdentityNumber }}</td>\r\n            <td>{{ item.Email }}</td>\r\n            <td>{{ item.Phonenumber }}</td>\r\n            <td>{{ item.BankAccountNumber }}</td>\r\n            <td>{{ item.BankAccountName}}</td>\r\n            <td>{{ item.Adress }}</td>\r\n            <td>{{ item.FacultyName }}</td>\r\n            <td>{{ item.Class}}</td>\r\n            <td>{{ item.EducationProgram }}</td>       \r\n            <td>{{ item.ClassificationName }}</td>       \r\n   