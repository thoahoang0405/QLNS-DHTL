nal sticky-right-top\"\r\n              colspan=\"12\"\r\n              style=\"min-width: 100px; text-align: center\"\r\n            >\r\n              <div class=\"th-item\">\r\n                <span class=\"table-text\">Chức năng </span>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr ref=\"row\" v-for=\"item in list\" :key=\"item.id\" @dblclick=\"btnEditStudent(item)\" >\r\n            <td\r\n              ref=\"rowCheck\"\r\n              class=\"checkbox sticky-left\"\r\n              style=\"text-align: center\"\r\n              colspan=\"1\"\r\n            >\r\n              <input\r\n                ref=\"check\"\r\n                class=\"check-item\"\r\n                type=\"checkbox\"\r\n                style=\"width: 18px; height: 18px\"\r\n              />\r\n            </td>\r\n            <td class=\"text-center\">{{ item.id }}</td>\r\n            <td>{{ item.msv }}</td>\r\n            <td>{{ item.ten }}</td>\r\n            <td>{{ item.gioitinh }}</td>\r\n            <td>{{ item.ngaysinh }}</td>\r\n             <td>{{ item.cmnd }}</td>\r\n            <td>{{ item.email }}</td>\r\n            <td>{{ item.sodt }}</td>\r\n            <td>{{ item.sotaikhoan }}</td>\r\n            <td>{{ item.diachi }}</td>\r\n            <td>{{ item.khoa }}</td>\r\n            <td>{{ item.kyhoc }}</td>\r\n            <td>{{ item.khenthuong }}</td>\r\n            <td>{{ item.kyluat }}</td>\r\n            <td>{{ item.hocbong }}</td>\r\n            <td>{{ item.trangthai }}</td>\r\n            <td>{{ item.tendangnhap }}</td>\r\n            <td>{{ item.matkhau }}</td>\r\n            <td\r\n              ref=\"func\"\r\n              class=\"td-item-final td-func sticky-right\"\r\n              style=\"position: sticky; right: 0; background-color: #fff\"\r\n              colspan=\"12\"\r\n            >\r\n              <div class=\"edit-text\"></div>\r\n              <div class=\"icon icon-edit\" @click=\